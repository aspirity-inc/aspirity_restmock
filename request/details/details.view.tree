$aspirity_restmock_request_details $mol_deck
	request $aspirity_restmock_request
	items /
		<= headers_item *
			title <= headers_title @ \Headers
			Content <= Headers $mol_view sub / <= headers \Headers
		<= query_item *
			title <= query_title @ \Query
			Content <= Query $mol_view sub / <= query \Query
		<= body_item *
			title <= body_title @ \Body
			Content <= Body $mol_view sub / <= body \Body
